{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "image",
        "hostNetwork",
        "resources",
        "additionalLabels",
        "podAnnotations",
        "nodeSelector",
        "tolerations",
        "affinity",
        "env",
        "icmpSupport",
        "connector"
    ],
    "properties": {
        "image": {
            "type": "object",
            "default": {},
            "title": "The image Schema",
            "properties": {
                "repository": {
                    "type": "string",
                    "default": "",
                    "title": "The repository Schema",
                    "examples": [
                        "twingate/connector"
                    ]
                },
                "tag": {
                    "type": "integer",
                    "default": 0,
                    "title": "The tag Schema",
                    "examples": [
                        1
                    ]
                },
                "pullPolicy": {
                    "type": "string",
                    "default": "",
                    "title": "The pullPolicy Schema",
                    "examples": [
                        "Always"
                    ]
                }
            },
            "examples": [{
                "repository": "twingate/connector",
                "tag": 1,
                "pullPolicy": "Always"
            }]
        },
        "hostNetwork": {
            "type": "boolean",
            "default": false,
            "title": "Enable hostnetwork on pod",
            "examples": [
                false
            ]
        },
        "env": {
            "type": "object",
            "default": {},
            "title": "The env Schema",
            "required": [],
            "properties": {},
            "examples": [{}]
        },
        "icmpSupport": {
            "type": "object",
            "default": {},
            "title": "The icmpSupport Schema",
            "required": [
                "enabled"
            ],
            "properties": {
                "enabled": {
                    "type": "boolean",
                    "default": false,
                    "title": "The enabled Schema",
                    "examples": [
                        false
                    ]
                }
            },
            "examples": [{
                "enabled": false
            }]
        },
        "connector": {
            "type": "object",
            "default": {},
            "title": "The connector Schema",
            "required": [
                "network"
            ],
            "properties": {
                "logLevel": {
                    "type": "string",
                    "default": "",
                    "enum": ["error", "warning", "info", "debug"],
                    "title": "The logLevel Schema",
                    "examples": [
                        "error"
                    ]
                },
                "logAnalytics": {
                    "type": "string",
                    "default": "",
                    "enum": ["v1", "v2"],
                    "title": "The logAnalytics Schema",
                    "examples": [
                        "v2"
                    ]
                },
                "network": {
                    "type": "string",
                    "default": null,
                    "title": "The network Schema",
                    "examples": [
                        null
                    ]
                },
                "url": {
                    "type": "string",
                    "default": "",
                    "title": "The url Schema",
                    "examples": [
                        "twingate.com"
                    ]
                },
                "accessToken": {
                    "type": "string",
                    "default": null,
                    "title": "The accessToken Schema",
                    "examples": [
                        null
                    ]
                },
                "refreshToken": {
                    "type": "string",
                    "default": null,
                    "title": "The refreshToken Schema",
                    "examples": [
                        null
                    ]
                },
                "existingSecret": {
                    "type": "string",
                    "default": null,
                    "title": "The existingSecret Schema",
                    "examples": [
                        null
                    ]
                },
                "dnsServer": {
                    "type": "string",
                    "default": null,
                    "title": "The dnsServer Schema",
                    "examples": [
                        null
                    ]
                }
            },
            "oneOf": [
                {
                    "required": [
                        "accessToken",
                        "refreshToken"
                    ]
                },
                {
                    "required": [
                        "existingSecret"
                    ]
                }
            ],
            "examples": [{
                "logLevel": "error",
                "logAnalytics": "v2",
                "network": null,
                "url": "twingate.com",
                "accessToken": null,
                "refreshToken": null,
                "existingSecret": null,
                "dnsServer": null
            }]
        }
    },
    "examples": [{
        "image": {
            "repository": "twingate/connector",
            "tag": 1,
            "pullPolicy": "Always"
        },
        "hostNetwork": false,
        "resources": {
            "requests": {
                "cpu": "50m",
                "memory": "200Mi"
            }
        },
        "additionalLabels": {},
        "podAnnotations": {},
        "nodeSelector": {},
        "tolerations": [],
        "affinity": {},
        "env": {},
        "icmpSupport": {
            "enabled": false
        },
        "connector": {
            "logLevel": "error",
            "logAnalytics": "v2",
            "network": null,
            "url": "twingate.com",
            "accessToken": null,
            "refreshToken": null,
            "existingSecret": null,
            "dnsServer": null
        }
    }]
}
